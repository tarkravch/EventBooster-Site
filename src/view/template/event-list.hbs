{{#each this}}

	<li class='cardset__item' data-event-id='{{id}}'>
		<img src='{{img.url}}' alt='{{name}}' class='cardset__img' />
		<h2 class='cardset__name'>{{name}}</h2>
		<p class='cardset__date'>{{dates.start.localDate}}</p>
		{{#if _embedded.venues.0.name}}
			<a
				data-map='map'
				class='cardset__place'
				target='_blank'
				href='https://www.google.com/maps/search/?api=1&query={{_embedded.venues.0.location.latitude}},{{_embedded.venues.0.location.longitude}}&query_place_id={{_embedded.venues.0.id}}'
			>{{_embedded.venues.0.name}}</a>
		{{/if}}
		<div class='cardset__rect'></div>

	</li>
{{/each}}